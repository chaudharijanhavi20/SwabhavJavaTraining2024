package calci;

public class CalculatorTest {
  public static void main(String[] args) {
  int a=20;
  int b=10;
  
   int sum = a+b;
   System.out.println("Addition ="+sum);
   
   int sub=a-b;
   System.out.println("Substraction= "+sub);
   
   int mul=a*b;
   System.out.println("Multiplication = "+mul);
   
   int div=a/b;
   System.out.print("Divsion = "+div);
  }
   
}
