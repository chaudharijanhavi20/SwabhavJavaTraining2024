package ArraysQuestion;

public class CountAndPrintEvenNumbers {
public static void main(String[] args) {
	int array[]=new int[] {1,4,3,2,67,56,88};
	int evenCount=countEvenNumber(array);
	System.out.println("Numbers of even numbers "+evenCount);
}
public static int countEvenNumber(int[] array1) {
	int count=0;
	for(int num:array1) {
		if((num &1)==0) {
			count++;
		}
	}
	return count;
}
}
